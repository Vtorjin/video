{
  "name": "video",
  "version": "0.0.0",
  "main": "out/src/main/index.js",
  "author": "victor jin",
  "description": "this is a video desk app built with angular & electron",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "electron:dev": "chcp 65001 && electron .",
    "electron": "electron",
    "electron:serve-tsc": "tsc -p tsconfig.node.json",
    "electron:serve": "wait-on tcp:4200 -w && npm run electron:serve-tsc && chcp 65001 && electron . --watch --serve",
    "electron:hot": "electron-reloader .",
    "electron:local": "npm run build:prod && electron .",
    "build": "npm run electron:serve-tsc && ng build --base-href ./",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "build:win": "npm run build:prod && electron-builder build --publish=never",
    "build:web": "ng build ",
    "web:build": "npm run build -- -c web-production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev": "npm-run-all -p electron:serve  start"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.1.0",
    "@angular/cdk": "16.2.1",
    "@angular/common": "^16.1.0",
    "@angular/compiler": "^16.1.0",
    "@angular/core": "^16.1.0",
    "@angular/forms": "^16.1.0",
    "@angular/material": "16.2.1",
    "@angular/platform-browser": "^16.1.0",
    "@angular/platform-browser-dynamic": "^16.1.0",
    "@angular/router": "^16.1.0",
    "asynckit": "^0.4.0",
    "axios": "^1.4.0",
    "combined-stream": "^1.0.8",
    "delayed-stream": "1.0.0",
    "electron-updater": "^5.3.0",
    "follow-redirects": "1.15.2",
    "form-data": "^4.0.0",
    "g": "^2.0.1",
    "mime-db": "1.52.0",
    "mime-types": "2.1.35",
    "proxy-from-env": "1.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.1.6",
    "@angular/cli": "~16.1.6",
    "@angular/compiler-cli": "^16.1.0",
    "@electron/notarize": "^1.2.3",
    "@types/jasmine": "~4.3.0",
    "@types/node": "18.15.11",
    "@types/ssh2": "^1.11.13",
    "electron": "^22.3.4",
    "electron-builder": "^23.6.0",
    "electron-chrome-extensions": "^3.10.1",
    "electron-log": "^4.4.8",
    "electron-reloader": "^1.2.3",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "less-loader": "^11.1.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.0",
    "ssh2": "^1.14.0",
    "ts-node": "^10.9.1",
    "typescript": "~5.1.3",
    "wait-on": "^7.0.1"
  }
}